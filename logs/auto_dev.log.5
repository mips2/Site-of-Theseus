            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }
        .memory-card.flipped {
            background: #6a5acd;
            color: white;
        }
        .memory-card.matched {
            background: #4caf50;
            color: white;
            pointer-events: none;
        }
