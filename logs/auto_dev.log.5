document.getElementById('submit-action').addEventListener('click', function() {
    const userInput = document.getElementById('user-input').value;
    if (userInput.trim() === '') return;

    const storyText = document.getElementById('story-text');
    const history = document.getElementById('history');

    // Simulate AI response (for now, just echo the input)
    const aiResponse = `You ${userInput}. The AI Dungeon Master nods thoughtfully.`;

    // Update story text
    storyText.textContent = aiResponse;

    // Add to history
    const historyEntry = document.createElement('p');
    historyEntry.textContent = `You: ${userInput}`;
    history.appendChild(historyEntry);

    const aiHistoryEntry = document.createElement('p');
    aiHistoryEntry.textContent = `AI: ${aiResponse}`;
    history.appendChild(aiHistoryEntry);

    // Clear input
    document.getElementById('user-input').value = '';

    // Scroll history to bottom
    history.scrollTop = history.scrollHeight;
});
