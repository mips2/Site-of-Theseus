```javascript
document.addEventListener('DOMContentLoaded', () => {
    const playerEmoji = document.getElementById('player-emoji');
    const enemyEmoji = document.getElementById('enemy-emoji');
    const playerHealthDisplay = document.getElementById('player-health');
    const enemyHealthDisplay = document.getElementById('enemy-health');
    const battleResult = document.getElementById('battle-result');
    const attackBtn = document.getElementById('attack-btn');
    const defendBtn = document.getElementById('defend-btn');
    const specialBtn = document.getElementById('special-btn');

    let playerHealth = 100;
    let enemyHealth = 100;

    attackBtn.addEventListener('click', () => {
        const damage = Math.floor(Math.random() * 20) + 1;
        enemyHealth -= damage;
        updateHealth();
        animateAttack(playerEmoji, enemyEmoji);
        if (enemyHealth <= 0) {
            endBattle('Player Wins!');
        } else {
            enemyAttack();
        }
    });

    defendBtn.addEventListener('click', () => {
        const defense = Math.floor(Math.random() * 15) + 1;
        playerHealth += defense;
        if (playerHealth > 100) playerHealth = 100;
        updateHealth();
        animateDefend(playerEmoji);
        enemyAttack();
    });

    specialBtn.addEventListener('click', () => {
        const specialDamage = Math.floor(Math.random() * 30) + 1;
        enemyHealth -= specialDamage;
        updateHealth();
        animateSpecial(playerEmoji, enemyEmoji);
        if (enemyHealth <= 0) {
            endBattle('Player Wins!');
        } else {
            enemyAttack();
        }
    });

    function enemyAttack() {
        const damage = Math.floor(Math.random() * 20) + 1;
        playerHealth -= damage;
        updateHealth();
        animateAttack(enemyEmoji, playerEmoji);
        if (playerHealth <= 0) {
            endBattle('Enemy Wins!');
        }
    }

    function updateHealth() {
        playerHealthDisplay.textContent = Math.max(playerHealth, 0);
        enemyHealthDisplay.textContent = Math.max(enemyHealth, 0);
    }

    function animateAttack(attacker, target) {
        attacker.style.transform = 'translateX(-20px)';
        target.style.transform = 'translateX(20px)';
        setTimeout(() => {
            attacker.style.transform = 'translateX(0)';
            target.style.transform = 'translateX(0)';
        }, 200);
    }

    function animateDefend(defender) {
        defender.style.transform = 'scale(1.2)';
        setTimeout(() => {
            defender.style.transform = 'scale(1)';
        }, 200);
    }

    function animateSpecial(attacker, target) {
        attacker.style.transform = 'rotate(360deg)';
        target.style.transform = 'scale(1.5)';
        setTimeout(() => {
            attacker.style.transform = 'rotate(0deg)';
            target.style.transform = 'scale(1)';
        }, 500);
    }

    function endBattle(result) {
        battleResult.textContent = result;
        attackBtn.disabled = true;
        defendBtn.disabled = true;
        specialBtn.disabled = true;
    }
});
