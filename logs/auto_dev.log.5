    <script>
        const color1 = document.getElementById('color1');
        const color2 = document.getElementById('color2');
        const resultColor = document.getElementById('result-color');
        const resultText = document.getElementById('result-text');

        function mixColors() {
            const color1Value = color1.value;
            const color2Value = color2.value;

            const r1 = parseInt(color1Value.slice(1, 3), 16);
            const g1 = parseInt(color1Value.slice(3, 5), 16);
            const b1 = parseInt(color1Value.slice(5, 7), 16);

            const r2 = parseInt(color2Value.slice(1, 3), 16);
            const g2 = parseInt(color2Value.slice(3, 5), 16);
            const b2 = parseInt(color2Value.slice(5, 7), 16);

            const mixedR = Math.round((r1 + r2) / 2);
            const mixedG = Math.round((g1 + g2) / 2);
            const mixedB = Math.round((b1 + b2) / 2);

            const mixedColor = `#${mixedR.toString(16).padStart(2, '0')}${mixedG.toString(16).padStart(2, '0')}${mixedB.toString(16).padStart(2, '0')}`;

            resultColor.style.backgroundColor = mixedColor;
            resultText.textContent = `Mixed Color: ${mixedColor}`;
        }

        color1.addEventListener('input', mixColors);
        color2.addEventListener('input', mixColors);

        mixColors(); // Initial call to set the default mixed color
    </script>
</body>
