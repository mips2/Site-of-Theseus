            <p>{{ post.content }}</p>
            <p>Likes: {{ post.likes }}</p>
            {% if current_user.is_authenticated %}
                <form method="POST" action="{{ url_for('like_post', post_id=post.id) }}">
                    <button type="submit">Like</button>
                </form>
            {% endif %}
        </div>
    {% endfor %}
    <p>{{ get_flashed_messages()[0] }}</p>
