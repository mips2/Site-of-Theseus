        <form method="POST" action="{{ url_for('create_post') }}">
            <textarea name="content" placeholder="What's on your mind?" required></textarea>
            <br>
            <button type="submit">Post</button>
        </form>
    {% else %}
        <p><a href="{{ url_for('login') }}">Login</a></p>
    {% endif %}
    <h2>Recent Posts</h2>
    {% for post in posts %}
